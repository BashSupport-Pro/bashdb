#!/bin/sh
# -*- shell-script -*-
#$Id: setshow.tests,v 1.2 2008/10/20 17:39:13 rockyb Exp $

[ -z "$srcdir" ] && export srcdir=$PWD
[ -z "$builddir" ] && export builddir=$PWD
[ -z "$top_builddir" ] && export top_builddir=$builddir/..

cmdfile="$top_srcdir/test/data/setshow.cmd"
debugged_script="$top_builddir/test/example/dbg-test2.sh"

$SH ${top_builddir}/bashdb -B --no-init -q -L ${top_builddir} -x $cmdfile $debugged_script testarg1 testarg2
