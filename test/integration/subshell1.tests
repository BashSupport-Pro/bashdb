#!/bin/sh
# -*- shell-script -*-
#$Id: subshell1.tests,v 1.1 2008/08/22 19:25:06 rockyb Exp $

[ -z "$srcdir" ] && export srcdir=$PWD
[ -z "$builddir" ] && export builddir=$PWD
[ -z "$top_builddir" ] && export top_builddir=$builddir/../..

cmdfile=${top_srcdir}/test/data/subshell1.cmd
debugged_script="${top_srcdir}/test/example/subshell.sh"

$SH ${top_builddir}/bashdb -B -q -L ${top_builddir} -x $cmdfile $debugged_script
