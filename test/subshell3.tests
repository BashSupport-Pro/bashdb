#!/bin/sh
#$Id: subshell3.tests,v 1.1 2006/01/02 23:34:26 rockyb Exp $

if test -z $srcdir  ; then
  srcdir=`pwd`
fi

if test -z $top_builddir ; then
  top_builddir=..
fi

cmdfile=subshell3.cmd
debugged_script=subshell.sh

$BASH ${TOP_BUILDDIR}bashdb -B -q -L .. -x $cmdfile $debugged_script

