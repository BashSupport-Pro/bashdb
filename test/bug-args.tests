#!/bin/sh
#$Id: bug-args.tests,v 1.1 2006/11/29 23:12:51 rockyb Exp $

if test -z $srcdir  ; then
  srcdir=`pwd`
fi

if test -z $builddir  ; then
  builddir=`pwd`
fi

cmdfile="$srcdir/bug-args.cmd"
debugged_script="$builddir/bug-args.sh"

$BASH ${TOP_BUILDDIR}bashdb -B -q -L .. -x $cmdfile $debugged_script

