if test -z $srcdir  ; then
  srcdir=`pwd`
fi

if test -z $top_builddir ; then
  top_builddir=..
fi

cmdfile="$srcdir/subshell4.cmd"
debugged_script="$srcdir/subshell.sh"

$BASH $top_builddir/bashdb -B -q -L $d -x $cmdfile $debugged_script

