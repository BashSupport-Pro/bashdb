#!@BASH_PROG@
# -*- shell-script -*-

test_print_frame()
{
    typeset result='bogus'

    function _Dbg_msg {
	msg=$1
    }
    
    msg=''
    _Dbg_print_frame '->' 0 '' dbg-cmds.inc 2 ''

    assertEquals "->0 in file \`dbg-cmds.inc' at line 2" "$msg"
}

top_srcdir=@top_builddir@
. ${top_srcdir}/lib/stack.inc

# load shunit2
srcdir=@srcdir@
srcdir=${srcdir}/
. ${srcdir}/shunit2

