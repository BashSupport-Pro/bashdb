#!@BASH_PROG@
# -*- shell-script -*-

test_preserve_set_opts()
{
    set -u
    old_opts=$-
    . ${top_builddir}/bashdb-trace -L ${top_builddir}
    assertEquals $old_opts $-
}

# load shunit2
top_builddir=@top_builddir@
. ${top_builddir}/command/tracefn.cmd

srcdir=@srcdir@
srcdir=${srcdir}/
. ${srcdir}/shunit2

