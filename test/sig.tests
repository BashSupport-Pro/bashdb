[ -z "$srcdir" ] && export srcdir=$PWD
[ -z "$builddir" ] && export builddir=$PWD
[ -z "$top_builddir" ] && export top_builddir=$builddir/..

cmdfile=${srcdir}/data/sig.cmd
debugged_script=sig.sh

$BASH ${top_builddir}/bashdb -B -q -L .. -x $cmdfile $debugged_script
